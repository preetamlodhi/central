<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.bachat.central.dao.Address" table="address" schema="" catalog="bachat">
        <id name="id">
            <column name="id" sql-type="bigint unsigned" not-null="true"/>
        </id>
        <property name="line1">
            <column name="line1" sql-type="varchar" length="50"/>
        </property>
        <property name="line2">
            <column name="line2" sql-type="varchar" length="50"/>
        </property>
        <property name="city">
            <column name="city" sql-type="varchar" length="20" not-null="true"/>
        </property>
        <property name="state">
            <column name="state" sql-type="varchar" length="20" not-null="true"/>
        </property>
        <property name="contactNumber">
            <column name="contact_number" sql-type="bigint unsigned" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>